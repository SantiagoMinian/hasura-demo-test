{
  "name": "react-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@apollo/client": "3.7.0",
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@mui/material": "^5.10.9",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@testing-library/user-event": "13.5.0",
    "@types/jest": "27.5.2",
    "@types/node": "16.11.66",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.6",
    "apollo": "2.33.0",
    "apollo-utilities": "1.3.4",
    "graphql": "15.8.0",
    "graphql-ws": "5.11.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "typescript": "4.8.4",
    "web-vitals": "2.1.4"
  },
  "resolutions": {
    "graphql": "15.8.0",
    "**/graphql": "15.8.0"
  },
  "scripts": {
    "preinstall": "npx npm-force-resolutions",
    "postinstall": "rm -r node_modules/apollo-language-server/node_modules/graphql",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "graphql:schema:download:local": "apollo schema:download --endpoint=http://localhost:8080/v1/graphql graphql-schema.json --header \"X-Hasura-Admin-Secret: adminsecret\"",
    "graphql:codegen:generate": "apollo codegen:generate --localSchemaFile=graphql-schema.json --target=typescript --includes=src/**/*.ts --tagName=gql --addTypename --globalTypesFile=src/types/graphql/index.ts types",
    "graphql:codegen:local": "npm run graphql:schema:download:local && npm run graphql:codegen:generate"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
